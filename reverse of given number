#include <iostream>
using namespace std;

int main() {
    int num, rev = 0;
    cout << "Enter a number: ";
    cin >> num;

    // Reverse the number
    while (num != 0) {
        rev = rev * 10 + num % 10;
        num /= 10;
    }

    cout << "Reverse of the number is: " << rev;
    return 0;
}
